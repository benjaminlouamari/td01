package java_td;

import java.util.Scanner;

public class Td01 {

	public static void main(String[] args) {
		
		Scanner clavier = new Scanner(System.in);
		
		System.out.println("Entrez une température (en Farenheit):");
		double TEMPERATURE_F = clavier.nextDouble();
		
		clavier.close();
		
		double TEMPERATURE_C = (TEMPERATURE_F-32)*5/9;
		
		System.out.println("La température en degré Celcius: " + TEMPERATURE_C);
		
	}

}
